<div class="container" style="padding-top: 10px; padding-left: 50px;">
  <% if flash[:notice] %>
    <div class="alert alert-info" role="alert" id="notice">
      <%= flash[:notice] %>
    </div>
  <% elsif flash[:warning] %>
    <div class="alert alert-danger" role="alert" id="warning">
      <%= flash[:warning] %>
    </div>
  <% end %>
</div>
