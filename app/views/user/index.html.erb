<div class = "user-info-wrapper">
  <h2>Profile</h2>
  <div class = "user-info">
    <% @user[0] do |user|%>
      <h4><% user.name %></h4>
      <h4>Placeholder Address</h4>
      <h4>Placeholder email</h4>
      <h4>Placeholder phone</h4>
      <h4>Placeholder payment</h4>
    <% end %> %>

  </div>
</div>

<div class = "cart-wrapper">
  <h3>Current Cart</h3>
  <div class = current-cart>
    <%= link_to "See Cart",view_cart_path %>
  </div>
</div>